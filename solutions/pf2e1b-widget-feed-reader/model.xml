<catalog>
    <widget>
        <name>pf2e1b-widget-feed-reader</name>
        <contextItemName>[BBHOST]</contextItemName>
        <properties>
            <property name="src">
                <value type="string">$(itemRoot)/index.html</value>
            </property>
            <property name="thumbnailUrl">
                <value type="string">$(itemRoot)/icon.png</value>
            </property>
            <property label="Title" name="title">
                <value type="string">pf2e1b-widget-feed-reader</value>
            </property>
            <property name="TemplateName">
                <value type="string">Standard_Widget</value>
            </property>
            <property label="Widget Chrome" name="widgetChrome" viewHint="select-one,manager,designModeOnly">
                <value type="string">$(contextRoot)/static/features/[BBHOST]/chromes/blank/chrome-blank.html</value>
            </property>
            <!--Custom Properties-->
            <property label="Feed URL" name="url" viewHint="manager,designModeOnly">
                <value type="string">http://blog.backbase.com/feed</value>
            </property>
        </properties>
        <tags>
            <tag type="section">FE Training Widgets</tag>
            <tag type="regular">training</tag>
        </tags>
    </widget>
</catalog>
